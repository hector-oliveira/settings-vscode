{
  /** FILE NESTING */
  "explorer.fileNesting.patterns": {
    "next.config.mjs": "next-env*",
    "package.json": "tsconfig*, vite*, .babel*, jest*, package-lock*, yarn*, pnpm-lock*, bun.lockb",
    "tailwind.config*": "tailwind.config*, postcss.config*",
    "App.tsx": "App.css, index.css, main.tsx, vite-env*",
    "eslint*": ".eslint*, .prettier*, prettier*, "
  },

  /** FILES ASSOCIATIONS */
  "symbols.files.associations": {
    "*.i.ts": "dts",
    "*.txt": "text",
    "*.hook.ts": "oxlint",
    "*.interface.ts": "exe",
    "*.test.jsx": "react-test",
    "*.test.ts": "ts-test",
    "*.test.tsx": "react-test",
    "tsconfig.ts": "tune"
  },

  /** FOLDERS ASSOCIATIONS */
  "symbols.folders.associations": {
    ".pnpm": "folder-yellow",
    "@types": "folder-blue",
    "build": "folder-purple-outline",
    "builder": "folder-purple-outline",
    "class": "folder-red",
    "data": "folder-database",
    "functions": "folder-blue",
    "hooks": "folder-target",
    "javascript": "folder-yellow-code",
    "js": "folder-yellow-code",
    "nodemon": "folder-green",
    "services": "folder-yellow",
    "tests": "folder-red-code",
    "themes": "folder-blue-code",
    "ts": "folder-blue-code",
    "typescript": "folder-blue-code",
    "pages": "folder-app"
  },

  /** GENERAL CONFIG */
  "breadcrumbs.enabled": false,
  "codeium.enableCodeLens": false,
  "cSpell.language": "en,pt-BR,pt",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorStyle": "block-outline",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.lineHeight": 1.8,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "gutter",
  "editor.rulers": [80, 120],
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.stickyScroll.enabled": false,
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "symbols.hidesExplorerArrows": false,
  "terminal.integrated.defaultProfile.windows": "Windows PowerShell",
  "terminal.integrated.fontFamily": "JetBrainsMono NF",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "bold",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.commandCenter": false,
  "window.doubleClickIconToClose": true,
  "window.menuBarVisibility": "hidden",
  "workbench.activityBar.location": "hidden",
  "workbench.colorTheme": "Min Dark",
  "workbench.editor.editorActionsLocation": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editor.labelFormat": "short",
  "workbench.iconTheme": "symbols",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.visible": false,
  "workbench.tree.indent": 14,

  /* CUSTOM OBJECT CONFIG */
  "workbench.colorCustomizations": {
    "editor.background": "#020202",
    "editor.lineHighlightBorder": "#000000",
    "editorGroupHeader.tabsBackground": "#000000",
    "editorGroupHeader.border": "#000000",
    "tab.activeBorder": "#000000",
    "tab.activeForeground": "#f5f5f5",
    "tab.border": "#000000",
    "editorGutter.background": "#020202",
    "list.activeSelectionBackground": "#494949",
    "list.activeSelectionIconForeground": "#04dd66",
    "scrollbar.shadow": "#000000",
    "list.hoverBackground": "#2c2c2c",
    "list.hoverForeground": "#f9f9f9",
    "panel.background": "#010101",
    "panelTitle.activeForeground": "#F1F1F1",
    "panelTitle.inactiveForeground": "#696969",
    "sideBar.background": "#060606",
    "sideBar.foreground": "#A9A9A9",
    "sideBarTitle.foreground": "#fcfcfc",
    "tab.activeBackground": "#060606",
    "tab.activeBorderTop": "#f9cb00",
    "tab.hoverBackground": "#363636",
    "tab.inactiveBackground": "#161616",
    "tab.inactiveForeground": "#bebebe",
    "tab.hoverForeground": "#f5f5f5",
    "terminal.background": "#050505",
    "terminal.foreground": "#cccccc",
    "terminal.tab.activeBorder": "#F94700",
    "titleBar.activeBackground": "#000000",
    "welcomePage.background": "#020202",
    "input.background": "#0e0d0d",
    "input.foreground": "#F1F1F1",
    "dropdown.background": "#0e0d0d",
    "dropdown.foreground": "#c9c9c9",
    "quickInput.background": "#0a0a0a",
    "editorWidget.background": "#111111",
    "editorWidget.foreground": "#A1A1A1",
    "input.placeholderForeground": "#b4b4b4"
  },

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },

  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],

  /* UNKNOWN WORDS CONFIG */
  "cSpell.userWords": [
    "auths",
    "codeium",
    "drawio",
    "esbenp",
    "geist",
    "hediet",
    "keystatic",
    "lockb",
    "oxlint",
    "quokka",
    "shadcn",
    "stylesheet",
    "trivago",
    "tseslint"
  ],

  "gitlens.graph.minimap.additionalTypes": [
    "localBranches",
    "stashes",
    "pullRequests"
  ]
}
